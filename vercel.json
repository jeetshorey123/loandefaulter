{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" },
    { "src": "api/predict.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/api/predict", "dest": "/api/predict.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
